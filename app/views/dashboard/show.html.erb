<% if @role == 'mentor' %>
  <h1>Welcome, Mentor!</h1>
  <%@mentors_data.each do |mentor|%>
    <h3><%=mentor.user_id%></h3>
  <%end%>
<% elsif @role == 'mentee' %>
  <h1>Welcome, Mentee!</h1>
  <%@mentees_data.each do |mentee|%>
    <h3><%=mentee.user_id%></h3>
  <%end%>
  <%= render partial: "shared/mentor_list", locals: {mentors_data: @mentors_data}%>
  <!-- Mentee Partial Views Here-->
<% elsif @role == 'admin' %>
  <h1>Welcome, Admin!</h1>
  <%@admins_data.each do |admin|%>
    <h3><%=admin.user_id%></h3>
  <%end%>
  <!-- Admin Partial Views Here-->
<% else %>
  <h1>Role not recognized.</h1>
<% end %>